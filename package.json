{
  "private": false,
  "name": "machi-cat-ui",
  "version": "0.0.2",
  "type": "module",
  "description": "UI Component Library with TailwindCSS",
  "scripts": {
    "build": "vue-tsc -b && vite build",
    "dev": "vite",
    "lint": "oxlint",
    "preview": "vite preview"
  },
  "main": "dist/machi-components.cjs",
  "module": "dist/machi-components.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/machi-components.cjs",
    "dist/machi-components.js",
    "dist/machi-plugin.cjs",
    "dist/machi-plugin.js",
    "dist/index.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "oxlint": "^0.15.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue-tsc": "^2.1.10"
  },
  "peerDependencies": {
    "tailwindcss": "^3.0.0",
    "vue": "^3.0.0"
  },
  "keywords": [
    "vue",
    "components",
    "ui-library",
    "typescript",
    "tailwindcss"
  ],
  "author": {
    "name": "Terry Chen",
    "email": "zxtw17985321@gmail.com",
    "url": "https://github.com/terry90918"
  },
  "license": "MIT",
  "homepage": "https://github.com/terry90918/web-machi-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terry90918/web-machi-ui.git"
  },
  "bugs": {
    "url": "https://github.com/terry90918/web-machi-ui/issues"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/machi-components.js",
      "require": "./dist/machi-components.cjs"
    },
    "./plugin": {
      "import": "./dist/machi-plugin.js",
      "require": "./dist/machi-plugin.umd.cjs"
    }
  }
